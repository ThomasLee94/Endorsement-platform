<div class="card" id="{{this._id}}" >
  <div class="card-block">
    <h4 class="card-name">{{this.name}}</h4>
    <p class="card-company">{{this.company}}</p>
    <button onclick="upVote('{{this.skillId}}', '{{this._id}}')">Upvote</button>
    <p id="upVotes-{{this._id}}">{{this.upVotes}}</p>
  </div>
</div>
